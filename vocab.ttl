@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vs: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix o: <http://omeka.org/s/vocabs/o#> .
@prefix gi: <https://jardindesconnaissances.univ-paris8.fr/onto/gi#> .
@prefix teach: <http://linkedscience.org/teach/ns#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix schema: <http://schema.org/>.

<https://jardindesconnaissances.univ-paris8.fr/onto/gi> a owl:Ontology ;
    dcterms:title "Governance de l'Internet" .

gi:NetNeutrality a rdfs:Class ;
    rdfs:label "Net Neutrality" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

gi:ProtectionDesDonnees a rdfs:Class ;
    rdfs:label "Protection des données" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

gi:DomainNameSystem a rdfs:Class ;
    rdfs:label "Domain Name System" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

gi:ICANN a rdfs:Class ;
    rdfs:label "Internet Corporation for Assigned Names and Numbers (ICANN)" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

gi:Cybersecurite a rdfs:Class ;
    rdfs:label "Cybersécurité" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .    

gi:Reglementations a rdfs:Class ;
    rdfs:label "Réglementation" ;
    rdfs:domain o:Resource ;
    owl:sameAs schema:Course ;    
    vs:term_status "experimental" .

gi:ModerationDeContenu a rdfs:Class ;
    rdfs:label "Modération de contenu" ;
    rdfs:domain o:Resource ;
    owl:sameAs schema:Course ;    
    vs:term_status "experimental" .



gi:HasMultilateralisme a rdf:Property ;
    rdfs:label "A comme Multilatéralisme" ;
    rdfs:comment "Relation avec Réglementation" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

gi:HasDecentralisation a rdf:Property ;
    rdfs:label "A comme Décentralisation" ;
    rdfs:comment "Relation avec Net Neutrality" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

gi:HasTransparence a rdf:Property ;
    rdfs:label "A comme Transparence" ;
    rdfs:comment "Relation avec Modération de contenu" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

gi:HasSecurite a rdf:Property ;
    rdfs:label "A comme Sécurité" ;
    rdfs:comment "Relation avec Cybersécurité" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

gi:HasCoordinationInternationale a rdf:Property ;
    rdfs:label "A comme Coordination internationale:" ;
    rdfs:comment "Relation avec Protection des données" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .




